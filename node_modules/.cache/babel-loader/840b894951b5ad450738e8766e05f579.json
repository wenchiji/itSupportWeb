CAAA,YAAA;AACA;AACA,MAAA,CAAA,CAAA,QAAA,CAAA,CAAA,SAAA,CAAA,YAAA;AACA,QAAA,QAAA,GAAA,IAAA,IAAA,GAAA,OAAA,EAAA,CADA,CACA;AAEA,OAHA;AAIA,KANA,CAAA;;AAQA,aAAA,QAAA,GAAA;AACA,MAAA,WAAA,GAAA,IAAA,IAAA,GAAA,OAAA,EAAA,CADA,CACA;;AACA,UAAA,WAAA,GAAA,QAAA,GAAA,OAAA,EAAA;AAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,QAAA,YAAA,CAAA,UAAA,CAAA,UAAA;AACA,QAAA,IAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,EAAA;AADA,SAAA;AAGA;AACA;AAEA;;;AACA,IAAA,MAAA,CAAA,WAAA,CAAA,QAAA,EAAA,IAAA;AACA;AAtDA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <el-container style=\"height: 100%; border: 1px solid #eee\">\r\n            <el-aside width=\"200px\" style=\"background-color: rgb(238, 241, 246)\">\r\n                <el-menu router :default-openeds=\"['2','3']\">\r\n                    <el-submenu v-for=\"(item,index) in $router.options.routes\" :index=\"index+''\" v-if=\"item.show\">\r\n                        <template slot=\"title\"><i class=\"el-icon-menu\"></i>{{item.name}}</template>\r\n                        <el-menu-item v-for=\"item2 in item.children\" :index=\"item2.path\"\r\n                                      :class=\"$route.path==item2.path?'is-active':''\">{{item2.name}}</el-menu-item>\r\n                    </el-submenu>\r\n                </el-menu>\r\n            </el-aside>\r\n            <el-container>\r\n                <el-header style=\"text-align: right; font-size: 12px\">\r\n                    <el-dropdown @command=\"logout\">\r\n                        <i class=\"el-icon-setting\" style=\"margin-right: 15px\"></i>\r\n                        <el-dropdown-menu slot=\"dropdown\">\r\n                            <el-dropdown-item >退出</el-dropdown-item>\r\n                        </el-dropdown-menu>\r\n                    </el-dropdown>\r\n                    <span>管理员</span>\r\n                </el-header>\r\n                <el-main>\r\n                    <router-view v-if=\"isRouterAlive\"></router-view>\r\n                </el-main>\r\n            </el-container>\r\n        </el-container>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: \"index\",\r\n        provide(){\r\n            return{\r\n                reload: this.reload\r\n            }\r\n        },\r\n        data() {\r\n            return {\r\n                isRouterAlive: true\r\n            };\r\n        },\r\n        methods:{\r\n            reload() {\r\n                this.isRouterAlive = false;\r\n                this.$nextTick(function () {\r\n                    this.isRouterAlive = true;\r\n                });\r\n            },\r\n            logout(){\r\n                localStorage.removeItem('eleToken');\r\n                localStorage.removeItem('user');\r\n                // window.location.href=\"/logout\";\r\n                this.$router.push(\"/\")\r\n            }\r\n        },\r\n        mounted() {\r\n            var that = this\r\n            //设置超时退出\r\n            var lastTime = new Date().getTime();\r\n            var currentTime = new Date().getTime();\r\n            var timeOut = 30 * 60 * 1000; //设置超时时间： 10分\r\n\r\n            $(function(){\r\n                /* 鼠标移动事件 */\r\n                $(document).mouseover(function(){\r\n                    lastTime = new Date().getTime(); //更新操作时间\r\n\r\n                });\r\n            });\r\n\r\n            function testTime(){\r\n                currentTime = new Date().getTime(); //更新当前时间\r\n                if(currentTime - lastTime > timeOut){ //判断是否超时\r\n                    console.log(\"超时\");\r\n                    localStorage.removeItem(\"eleToken\")\r\n                    that.$router.push({\r\n                        path:\"/\"\r\n                    })\r\n                }\r\n            }\r\n\r\n            /* 定时器  间隔1秒检测是否长时间未操作页面  */\r\n            window.setInterval(testTime, 1000);\r\n        },\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/views"}]}